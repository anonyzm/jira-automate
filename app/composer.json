        }
    ],
    "require": {
        "ext-rdkafka": "*",
        "php": ">=8.2",
        "symfony/framework-bundle": "^7",
        "symfony/yaml": "^7",
        "symfony/config": "^7",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "^7",
        "symfony/property-info": "^7",
        "guzzlehttp/guzzle": "^7.7",
        "symfony/console": "^7",
        "symfony/runtime": "^7",
        "symfony/messenger": "^7",
        "spiral/tokenizer": "^3.15",
        "symfony/serializer": "^7",
        "ramsey/uuid": "^4.9",
        "doctrine/orm": "^3.0",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/dbal": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "symfony/test-pack": "^1.2",
        "phpstan/phpstan": "^2.1"
